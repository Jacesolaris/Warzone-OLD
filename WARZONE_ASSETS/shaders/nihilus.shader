models/players/nihilus/body_red
{
	cull	disable
    {
        map models/players/nihilus/body_red
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/nihilus/body_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
}

