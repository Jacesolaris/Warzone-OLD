models/weapons2/tor_sabers/zallow
{
	q3map_nolightmap
    {
        map models/weapons2/tor_sabers/zallow
    }
    {
        map models/weapons2/tor_sabers/enviro
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/weapons2/tor_sabers/zallow_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

