models/players/wardroid/wardroid
{
	cull	twosided
    {
        map models/players/wardroid/wardroid
        rgbGen lightingDiffuse
    }
    {
        map gfx/world/mc
        blendFunc GL_ONE GL_ONE
        rgbGen lightingDiffuse
