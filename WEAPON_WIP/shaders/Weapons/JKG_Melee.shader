models/weapons/lightsabers/CaelumSereno/caelumsereno
{
    {
        map models/weapons/lightsabers/CaelumSereno/caelumsereno
        rgbGen lightingDiffuse
    }
    {
        map models/weapons/lightsabers/CaelumSereno/caelumsereno_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map textures/common/env_chrome
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        detail
        tcGen environment
    }
    {
        map envmap_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        glow
        detail
        rgbGen lightingDiffuse
        alphaGen lightingSpecular
        tcGen environment
    }
}

